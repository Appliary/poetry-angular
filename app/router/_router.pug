header
    h1(icon="{{$root.__module.icon}}")
        | {{ $root.__module.name + ':module:title' | translate }}
    button( ng-repeat='(name, action) in $root.__module.toolbox', ng-click="action()" , style="position: absolute; right: 20px; top: 15px;")
        | {{ name + ':toolbox:' + $root.__module.name | translate }}
ng-include(src='$root.__module.templateUrl')
